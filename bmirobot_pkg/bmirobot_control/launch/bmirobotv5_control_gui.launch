<?xml version="1.0"?>
<launch>


  <include file="$(find bmirobotv5)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>



  <!-- Load joint controller configurations from YAML file to parameter server group_hand_controller  -->
  <rosparam file="$(find bmirobot_control)/config/bmirobot_control.yaml" command="load"/>
<!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/bmirobot" args="joint_state_controller group_controller group_hand_controller"/>

  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
    respawn="false" output="screen">
    <remap from="/joint_states" to="/bmirobot/joint_states" />
  </node>

</launch>
